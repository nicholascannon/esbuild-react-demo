{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react",
    "module": "ESNext",
    "rootDir": "src",
    "moduleResolution": "node",
    "sourceMap": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnreachableCode": false,
    "skipLibCheck": true
  }
}
